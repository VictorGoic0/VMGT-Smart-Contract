# Copy this file to .env and fill in the values.
# Never commit .env or real keys to version control.

# Sepolia testnet RPC URL (from Alchemy, Infura, or other provider)
SEPOLIA_RPC_URL=

# Private key of the deployer account (MetaMask export, no 0x prefix or with 0x both work)
SEPOLIA_PRIVATE_KEY=

# Etherscan API key for contract verification (optional)
# https://etherscan.io/myapikey
ETHERSCAN_API_KEY=
